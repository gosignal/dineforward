{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "preserve",
    "lib": ["dom", "es2017"],
    "moduleResolution": "node",
    "allowJs": true,
    "noEmit": true,
    "strict": false,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "baseUrl": "./",
    "paths": {
      "~gen/*": ["./__generated/*"],
      "~gql/*": ["graphql/*"],
      "~views/*": ["src/views/*"],
      "~theme/*": ["src/views/theme/*"],
      "~components/*": ["src/views/components/*"],
      "~assets/*": ["src/views/theme/assets/*"],
      "~containers/*": ["src/views/containers/*"],
      "~utils/*": ["src/utils/*"],
      "~config/*": ["config/*"],
      "~hooks/*": ["src/utils/hooks/*"],
      "~ctx/*": ["src/utils/contexts/*"],
      "~types/*": ["interfaces/*"]
    },
    "forceConsistentCasingInFileNames": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "exclude": ["apollo.config.js", "dist", ".next", "out", "next.config.js", "node_modules"],
  "include": [
    "**/*",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/pages/_app.tsx",
    "src/views/containers/old/Admin/Admin.jsx",
    "src/views/containers/old/Admin/Workspace/index.js"
  ]
}
